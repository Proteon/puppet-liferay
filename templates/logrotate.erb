# Managed by puppet
/opt/tomcat/sites/<%= @instance %>/logs/liferay.*.log {
    daily
    rotate <%= @rotate_number %>
    compress
    delaycompress
    nomissingok
}

/opt/tomcat/sites/<%= @instance %>/logs/liferay.*.xml {
    daily
    rotate <%= @rotate_number %>
    compress
    delaycompress
    nomissingok
}

